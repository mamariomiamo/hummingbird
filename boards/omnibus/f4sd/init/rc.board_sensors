#!/bin/sh
#
# Omnibus F4SD specific board sensors init
#------------------------------------------------------------------------------

adc start
# start GPS on UART6
gps start -d /dev/ttyS2

if ! mpu6000 -R 6 -s start
then
	# some boards such as the Hobbywing XRotor F4 G2 use the ICM-20602
	icm20602 -s -R 6 start
fi

bmp280 -s start
